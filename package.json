{
  "name": "apex-specs-generator",
  "private": false,
  "version": "1.0.0",
  "description": "This is a gulp task, which automatically generates any specification documents of Apex classes or triggers in markdown format.",
  "keywords": [
    "Salesforce",
    "Apex",
    "Tooling API"
  ],
  "author": "Takahito Miyamoto",
  "license": "MIT",
  "scripts": {
    "gulp:specs:apex:generate": "gulp generateApexSpecs && yarn prettier",
    "gulp:init": "gulp init",
    "prettier": "prettier \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,xml,yaml,yml}\" --write --loglevel log"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.13",
    "@babel/preset-env": "7.13.12",
    "@babel/register": "7.13.8",
    "@prettier/plugin-xml": "0.13.1",
    "@salesforce/eslint-config-lwc": "0.11.0",
    "@salesforce/eslint-plugin-aura": "1.4.0",
    "eslint": "7.23.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-shell": "0.8.0",
    "heat-sfdx-common": "0.1.14",
    "heat-sfdx-tooling": "0.0.3",
    "json2md": "1.10.0",
    "prettier": "2.2.1",
    "prettier-plugin-apex": "1.8.0",
    "uuid": "8.3.2"
  }
}
