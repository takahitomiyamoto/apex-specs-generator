{
  "name": "apex-specs-generator",
  "private": false,
  "version": "1.0.0",
  "description": "This is a gulp task, which automatically generates any specification documents of Apex classes or triggers in markdown format.",
  "keywords": [
    "Salesforce",
    "Apex",
    "Tooling API"
  ],
  "author": "Takahito Miyamoto",
  "license": "MIT",
  "scripts": {
    "gulp:specs:apex:generate": "gulp generateApexSpecs",
    "gulp:init": "gulp init",
    "prettier": "prettier \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,xml,yaml,yml}\" --write --loglevel log"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "@prettier/plugin-xml": "0.12.0",
    "@salesforce/eslint-config-lwc": "0.7.0",
    "@salesforce/eslint-plugin-aura": "1.4.0",
    "eslint": "7.10.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-shell": "0.8.0",
    "heat-sfdx-common": "0.1.14",
    "heat-sfdx-tooling": "0.0.2",
    "json2md": "1.8.0",
    "prettier": "2.1.2",
    "prettier-plugin-apex": "1.7.0",
    "uuid": "8.3.0"
  }
}
