{
  "name": "apex-specs-generator",
  "private": false,
  "version": "1.0.0",
  "description": "This is a gulp task, which automatically generates any specification documents of Apex classes or triggers in markdown format.",
  "keywords": [
    "Salesforce",
    "Apex",
    "Tooling API"
  ],
  "author": "Takahito Miyamoto",
  "license": "MIT",
  "scripts": {
    "gulp:specs:apex:generate": "gulp generateApexSpecs && yarn prettier",
    "gulp:init": "gulp init",
    "prettier": "prettier \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,xml,yaml,yml}\" --write --loglevel log"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.21.0",
    "@prettier/plugin-xml": "2.2.0",
    "@salesforce/eslint-config-lwc": "3.4.0",
    "@salesforce/eslint-plugin-aura": "2.1.0",
    "eslint": "8.36.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-shell": "0.8.0",
    "heat-sfdx-common": "0.3.1",
    "heat-sfdx-tooling": "0.2.1",
    "json2md": "2.0.0",
    "prettier": "2.8.4",
    "prettier-plugin-apex": "1.12.0",
    "uuid": "9.0.0"
  }
}
